{
  "name": "postcss-gradient-transparency-fix",
  "version": "1.0.2",
  "description": "PostCSS plugin to fix gradient transparency for Safari and older browsers",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "gradient",
    "transparent"
  ],
  "author": "Gilmore Davidson <gilmoreorless@gmail.com>",
  "license": "MIT",
  "repository": "gilmoreorless/postcss-gradient-transparency-fix",
  "bugs": {
    "url": "https://github.com/gilmoreorless/postcss-gradient-transparency-fix/issues"
  },
  "homepage": "https://github.com/gilmoreorless/postcss-gradient-transparency-fix",
  "dependencies": {
    "color": "^0.12.0",
    "postcss": "^5.0.14",
    "postcss-value-parser": "^3.3.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.1.0",
    "eslint-config-postcss": "^2.0.2",
    "mocha": "^3.0.1"
  },
  "scripts": {
